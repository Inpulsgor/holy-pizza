      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "lint-staged": {
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.+{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.js": "eslint --cache --fix"
